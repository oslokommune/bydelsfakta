{
  "name": "bydelsfakta",
  "version": "1.1.28",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "backend": "env-cmd -f ./.env.local nodemon server/server.js",
    "build:prod": "npm install && npm run lint && npm run test:unit && npm run build",
    "docker": "npm run build:prod && npm run docker:build",
    "docker:build": "docker build -t ghcr.io/oslokommune/bydelsfakta-frontend:$npm_package_version .",
    "docker:push": "docker push ghcr.io/oslokommune/bydelsfakta-frontend:$npm_package_version",
    "lint:style": "stylelint 'src/**/*.scss' 'src/**/*.vue'",
    "lint:style:fix": "stylelint 'src/**/*.scss' 'src/**/*.vue' --fix",
    "test:unit:watch": "vue-cli-service test:unit --watchAll"
  },
  "dependencies": {
    "@sentry/vue": "^6.7.2",
    "axios": "^0.21.2",
    "canvas": "^2.8.0",
    "canvg": "^3.0.7",
    "core-js": "^3.15.1",
    "d3": "^5.16.0",
    "d3-svg-legend": "^2.25.6",
    "d3-textwrap": "^3.0.0",
    "leaflet": "^1.7.1",
    "leaflet-gesture-handling": "^1.2.1",
    "nodemon": "^2.0.20",
    "save-svg-as-png": "^1.4.17",
    "vue": "^2.6.14",
    "vue-analytics": "^5.22.1",
    "vue-dragscroll": "^1.10.2",
    "vue-i18n": "^8.24.5",
    "vue-meta": "^2.4.0",
    "vue-resize": "^1.0.1",
    "vue-router": "^3.5.2",
    "vue-skip-to": "^1.0.6",
    "vue2-leaflet": "^2.7.0",
    "vuex": "^3.6.2",
    "xlsx": "^0.17.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.1",
    "babel-eslint": "^10.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.29.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.11.1",
    "prettier": "^2.3.1",
    "sass": "^1.35.1",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "vue-cli-plugin-i18n": "^2.1.1",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.14"
  }
}
